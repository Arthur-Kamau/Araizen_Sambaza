<Window x:Class="Sambaza.Ui_Trial_End_Warning"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sambaza"
        mc:Ignorable="d"
      Closing="Window_Closing"
        Title="Trial Warning" Height="250" Width="400">
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        <Label Content="You trial licence wil expire soon. " Margin="60,0,0,0"></Label>
        <Label Content="Visit www.Araizen.com/dashboard/purchase/Crypto "></Label>
        <Button Content="Okay" Name="Okay" Click="Okay_Click" Width="120" Background="Transparent" BorderBrush="Transparent" ></Button>
    </StackPanel>
</Window>
